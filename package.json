{
  "name": "object-collection",
  "version": "4.1.3",
  "description": "Break objects into pieces and control them from those pieces.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./dist/index.js",
      "import": "./index.js"
    },
    "./exports": {
      "types": "./exports.d.ts",
      "require": "./dist/exports.js",
      "import": "./exports.js"
    },
    "./lodash": {
      "types": "./lodash.d.ts",
      "require": "./dist/lodash.js",
      "import": "./lodash.js"
    }
  },
  "scripts": {
    "start": "npm run test",
    "prepublishOnly": "npm run build",
    "build": "npx tsc && npm run bundle && npm run test",
    "watch": "npx tsc --watch",
    "test": "node --loader=ts-node/esm bin/test.ts",
    "test-dev": "npx nodemon japa.js --watch 'tests/**' --ext 'ts'",
    "bundle": "npx tsup"
  },
  "repository": "https://github.com/trapcodeio/object-collection.git",
  "author": "trapcodeio",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@japa/assert": "^1.3.6",
    "@japa/runner": "^2.2.2",
    "@japa/spec-reporter": "^1.3.2",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "^4.17.6",
    "japa": "^4.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "object-collection",
    "object",
    "collection",
    "lodash-collection"
  ]
}
