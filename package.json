{
  "name": "object-collection",
  "version": "4.0.5",
  "description": "Break objects into pieces and control them from those pieces.",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./exports": {
      "import": "./dist/exports.mjs",
      "require": "./exports.js",
      "types": "./exports.d.ts"
    },
    "./lodash": {
      "import": "./dist/lodash.mjs",
      "require": "./lodash.js",
      "types": "./lodash.d.ts"
    }
  },
  "scripts": {
    "start": "npm run test",
    "prepublishOnly": "npm run build",
    "build": "npx tsc && npm run test",
    "watch": "npx tsc --watch",
    "test": "node japa.js",
    "test-dev": "npx nodemon japa.js --watch 'tests/**' --ext 'ts'",
    "esm": "npx tsup"
  },
  "repository": "https://github.com/trapcodeio/object-collection.git",
  "author": "trapcodeio",
  "license": "MIT",
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "japa": "^4.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "tsup": "^6.1.0",
    "typescript": "^4.7.3"
  },
  "keywords": [
    "object-collection",
    "object",
    "collection",
    "lodash-collection"
  ]
}
